<solver>
  
  <solutionClass>uk.ac.diamond.ss.PlannerSolution</solutionClass>
  <entityClass>uk.ac.diamond.ss.domain.Allocation</entityClass>
  <randomSeed>10</randomSeed>
   
   
  <scoreDirectorFactory>
    <scoreDefinitionType>HARD_SOFT_LONG</scoreDefinitionType>
    
    
    
    
    <scoreDrl>scheduling.drl</scoreDrl>
    <initializingScoreTrend>ONLY_DOWN</initializingScoreTrend>
    
      
    
  </scoreDirectorFactory>
  
  <constructionHeuristic>
    <constructionHeuristicType>FIRST_FIT</constructionHeuristicType>
  </constructionHeuristic>
  
  <localSearch>
    <changeMoveSelector>
      <selectionOrder>ORIGINAL</selectionOrder>
    </changeMoveSelector>
    
    
      
    
    <acceptor>
      <entityTabuSize>13</entityTabuSize>
    </acceptor>
    <forager>
      
    </forager>
  </localSearch>

  
  
    
      
    
    
      
      
    
  

  

</solver>